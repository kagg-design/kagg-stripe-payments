{
  "name": "kagg-design/kagg-stripe-payments",
  "description": "KAGG Stripe Payments",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "stripe"
  ],
  "homepage": "https://github.com/kagg-design/kagg-stripe-payments",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "KAGG Design",
      "email": "info@kagg.eu",
      "homepage": "https://kagg.eu/en/"
    }
  ],
  "support": {
    "issues": "https://kagg.eu"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.13.4",
    "phpcompatibility/php-compatibility": "^9.3.5",
    "phpcompatibility/phpcompatibility-wp": "^2.1.8",
    "wp-coding-standards/wpcs": "^3.2.0"
  },
  "autoload": {
    "psr-4": {
      "KAGG\\Stripe\\": "src/php",
      "KAGG\\Settings\\Abstracts\\": "src/php/Settings/Abstracts"
    }
  },
  "scripts": {
    "phpcs": "vendor/bin/phpcs --colors --standard=phpcs.xml"
  }
}
